# Discord Bot Configuration
# Replace with your actual bot token
TOKEN=your_discord_bot_token_here

# Optional: Set your bot's command prefix for legacy commands
PREFIX=!

# Optional: Set your server/guild ID for development
GUILD_ID=your_guild_id_here

# Channel ID for moderation logs
MOD_LOG_CHANNEL=your_mod_log_channel_id_here

# Dashboard Configuration
# Your Discord application client ID
CLIENT_ID=your_discord_client_id_here

# Your Discord application client secret
CLIENT_SECRET=your_discord_client_secret_here

# OAuth2 redirect URI (default: http://localhost:8124/auth/discord/callback)
REDIRECT_URI=http://localhost:8124/auth/discord/callback

# Dashboard port (default: 8124)
DASHBOARD_PORT=8124

# Session secret for dashboard authentication
SESSION_SECRET=change_this_to_a_random_string